{
  "name": "ai-code",
  "displayName": "ai-code",
  "description": "OpenAI Codex client",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ai-code.codexEdit"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-code.codexEdit",
        "title": "Edit Current File with OpenAI Codex"
      }
    ],
    "configuration": [
      {
        "title": "AI Code",
        "properties": {
          "ai-code.openAiApiKey": {
            "title": "OpenAI API Key",
            "markdownDescription": "Create an account at https://beta.openai.com/, then copy your API key from https://beta.openai.com/account/api-keys here. You may need to add a credit card.",
            "type": "string",
            "default": ""
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "openai": "^2.0.5"
  }
}
